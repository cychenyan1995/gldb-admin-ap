n) {\n      if (column) {\n        return util.formatDate.format(new Date(row[column.property]), 'yyyy-MM-dd');\n      }\n    },\n    createTimeFormatDate: function (row, column) {\n      if (row.createTime) {\n        return util.formatDate.format(new Date(row[column.property]), 'yyyy-MM-dd');\n      }\n    },\n    updateTimeFormatDate: function (row, column) {\n      if (row.updateTime) {\n        return util.formatDate.format(new Date(row[column.property]), 'yyyy-MM-dd');\n      }\n    }\n  },\n  mounted () {\n    this.getList();\n  }};\n\n</script>\n\n<style>\n  .el-tooltip__popper {\n    max-width: 400px;\n    line-height: 180%;\n  }\n</style>\n\n\n\n\n// WEBPACK FOOTER //\n// src/modules/mkmsg/page/Content.vue","var render = function () {var _vm=this;var _h=_vm.$createElement;var _c=_vm._self._c||_h;return _c('section',[_c('el-col',{staticClass:\"toolbar\",staticStyle:{\"padding-bottom\":\"0px\"},attrs:{\"span\":24}},[_c('el-form',{attrs:{\"inline\":true,\"model\":_vm.filters}},[_c('el-form-item',[_c('el-input',{attrs:{\"size\":\"small\",\"placeholder\":\"营销ID\"},model:{value:(_vm.filters.appMsgId),callback:function ($$v) {_vm.$set(_vm.filters, \"appMsgId\", $$v)},expression:\"filters.appMsgId\"}})],1),_vm._v(\" \"),_c('el-form-item',[_c('el-button',{attrs:{\"type\":\"primary\",\"size\":\"small\",\"icon\":\"el-icon-search\"},on:{\"click\":_vm.search}},[_vm._v(\"搜索\")])],1)],1)],1),_vm._v(\" \"),[_c('el-table',{directives:[{name:\"loading\",rawName:\"v-loading\",value:(_vm.listLoading),expression:\"listLoading\"}],staticStyle:{\"width\":\"100%\",\"border-color\":\"#DCDFE6\"},attrs:{\"data\":_vm.table.contents,\"default-sort\":{prop: 'createTime', order: 'descending'},\"border\":\"\",\"highlight-current-row\":\"\",\"max-height\":\"480\"}},[_c('el-table-column',{attrs:{\"type\":\"index\",\"width\":\"60\"}}),_vm._v(\" \"),_c('el-table-column',{attrs:{\"prop\":\"appMsgId\",\"label\":\"营销ID\",\"width\":\"120\"}}),_vm._v(\" \"),_c('el-table-column',{attrs:{\"prop\":\